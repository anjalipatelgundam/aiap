def calculate_average(scores):
    """
    Calculates the average of a list of scores.

    Args:
        scores (list): A list of numerical scores.

    Returns:
        float: The average of the scores.
    """
    if not scores:
        return 0.0
    return sum(scores) / len(scores)

def find_highest(scores):
    """
    Finds the highest score in a list.

    Args:
        scores (list): A list of numerical scores.

    Returns:
        int or float: The highest score.
    """
    if not scores:
        return None
    return max(scores)

def find_lowest(scores):
    """
    Finds the lowest score in a list.

    Args:
        scores (list): A list of numerical scores.

    Returns:
        int or float: The lowest score.
    """
    if not scores:
        return None
    return min(scores)

def process_scores(scores):
    """
    Processes a list of scores to find and print the average, highest, and lowest scores.

    Args:
        scores (list): A list of numerical scores.
    """
    avg = calculate_average(scores)
    highest = find_highest(scores)
    lowest = find_lowest(scores)

    print(f"Average: {avg}")
    print(f"Highest: {highest}")
    print(f"Lowest: {lowest}")

# --- Test Cases ---
print("Processing scores: [85, 90, 78, 92, 88]")
process_scores([85, 90, 78, 92, 88])
print("\nProcessing scores: [10, 20, 30, 40, 50]")
process_scores([10, 20, 30, 40, 50])
print("\nProcessing scores: [5]")
process_scores([5])
print("\nProcessing scores: []")
process_scores([])
